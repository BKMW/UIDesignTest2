                  x:Class="UIDesignTest2.Page1"
                  xmlns:pages="clr-namespace:UIDesignTest2">


    <MasterDetailPage.Master>
        <pages:MasterPage x:Name="Master" />
    </MasterDetailPage.Master>


    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:DetailPage />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>